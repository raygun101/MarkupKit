<LMTableView style="groupedTableView" estimatedSectionHeaderHeight="0" estimatedSectionFooterHeight="0">
    <sectionHeader title="Layout"/>

    <UITableViewCell textLabel.text="Horizontal Alignment" accessoryType="disclosureIndicator" value="horizontalAlignment"/>
    <UITableViewCell textLabel.text="Vertical Alignment" accessoryType="disclosureIndicator" value="verticalAlignment"/>
    <UITableViewCell textLabel.text="Anchor View" accessoryType="disclosureIndicator" value="anchorView"/>
    <UITableViewCell textLabel.text="Grid View" accessoryType="disclosureIndicator" value="gridView"/>
    <UITableViewCell textLabel.text="Periodic Table" accessoryType="disclosureIndicator" value="periodicTable"/>

    <?sectionBreak?>

    <sectionHeader title="Content and Navigation"/>

    <?case iOS?>
    <UITableViewCell textLabel.text="Scroll View" accessoryType="disclosureIndicator" value="scrollView"/>
    <UITableViewCell textLabel.text="Page View" accessoryType="disclosureIndicator" value="pageView"/>
    <?end?>

    <UITableViewCell textLabel.text="Custom Cell View" accessoryType="disclosureIndicator" value="customCellView"/>
    <UITableViewCell textLabel.text="Custom Section View" accessoryType="disclosureIndicator" value="customSectionView"/>

    <?case iOS?>
    <UITableViewCell textLabel.text="Custom Component View" accessoryType="disclosureIndicator" value="customComponentView"/>
    <?end?>

    <UITableViewCell textLabel.text="Collection View" accessoryType="disclosureIndicator" value="collectionView"/>

    <?case iOS?>
    <UITableViewCell textLabel.text="Web View" accessoryType="disclosureIndicator" value="webView"/>
    <UITableViewCell textLabel.text="Map View" accessoryType="disclosureIndicator" value="mapView"/>
    <?end?>

    <?sectionBreak?>

    <sectionHeader title="Selection Management"/>

    <UITableViewCell textLabel.text="Radio Buttons" accessoryType="disclosureIndicator" value="radioButtons"/>
    <UITableViewCell textLabel.text="Checkboxes" accessoryType="disclosureIndicator" value="checkboxes"/>

    <?sectionBreak?>

    <sectionHeader title="Effects"/>

    <UITableViewCell textLabel.text="Effect View" accessoryType="disclosureIndicator" value="effectView"/>

    <?case iOS?>
    <UITableViewCell textLabel.text="Animation" accessoryType="disclosureIndicator" value="animation"/>
    <?end?>
</LMTableView>
